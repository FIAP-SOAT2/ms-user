{
  "name": "clean-arch",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/server.js",
  "scripts": {
    "db:generate:migration": "npx prisma migrate dev -- --create-only",
    "db:generate:models": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "apply-db-migrations": "prisma migrate deploy",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "copy-swagger": "cp src/main/doc/swagger.yaml dist/main/doc",
    "build:watch": "npm run build -- --watch",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/' --exec 'ts-node src/server.ts' -e ts",
    "start:dev": "npm run build:watch\" \"npm run dev\"",
    "start:dev:win": "npx nodemon src/server.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache --coverage",
    "test:bdd": "cucumber-js test --format summary",
    "test:stage": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "schema": "src/infra/database/orm/prisma/schema.prisma"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.2.1",
    "@types/cors": "^2.8.13",
    "@types/cucumber": "^7.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.11.1",
    "cucumber": "^6.0.7",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "swagger-autogen": "^2.23.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.17",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@domain": "dist/domain",
    "@application": "dist/application",
    "@infra": "dist/infra",
    "@main": "dist/main"
  },
  "dependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@prisma/client": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "body-parser": "^1.20.2",
    "chai": "^5.0.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4",
    "swagger-ui": "^5.4.2",
    "swagger-ui-express": "^5.0.0"
  }
}
